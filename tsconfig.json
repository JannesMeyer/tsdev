{
    "compilerOptions": {
		"incremental": true,
		"sourceMap": false,
		"strict": true,
		"noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
        "target": "ES2020",
        "module": "ES2020",
        "esModuleInterop": true
    },
    "exclude": ["src"]
}
